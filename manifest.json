{
    "manifest_version": 3,
    "name": "Blue Archive Reset Reminder",
    "version": "1.0",
    "description": "Chrome extension to reminder about Blue Archive daily quests",
    "permissions": [
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  